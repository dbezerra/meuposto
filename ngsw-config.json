{
  "$schema": "https://angular.dev/assets/schema/service-worker-config.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/manifest.webmanifest"
        ],
        "urls": [
          "/assets/img/**",
          "/assets/luxand/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-cache",
      "urls": [
        "/api/portalprestador/**",
        "/rest/PORTALPRESTADOR/**"
      ],
      "cacheConfig": {
        "strategy": "cacheFirst",
        "maxSize": 100,
        "maxAge": "7d",
        "timeout": "10s"
      }
    },
    {
      "name": "images-cache",
      "urls": [
        "/assets/img/**",
        "/assets/luxand/**"
      ],
      "cacheConfig": {
        "strategy": "cacheFirst",
        "maxSize": 50,
        "maxAge": "30d",
        "timeout": "5s"
      }
    }
  ]
}


